<h2 id="heading-users" class="text-center">Viking Store Users</h2>
<br class="text-center">
  <%= link_to("Create a New User", new_user_path, :class => "btn btn-primary") %>
</br>

<table class="table table-striped table-bordered">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Joined</th>
    <th>City</th>
    <th>State</th>
    <th>Orders</th>
    <th>Last Order Date</th>
    <th>SHOW</th>
    <th>EDIT</th>
    <th>DELETE</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
        <td><%= product.id %></td>
        <td><%= product.name %></td>
        <td>$<%= product.price %></td>
        <td><%= link_to product.category.name, category_path(product.category.id) %></td>
        <td><%= link_to("SHOW", product_path(product)) %></td>
        <td><%= link_to("EDIT", edit_product_path(product)) %></td>
        <td><%= link_to("DELETE", product_path(product), method: :delete,
                                  data: {confirm: "You sure?"} ) %></td>
    </tr>
    <% end %>
  </table>
