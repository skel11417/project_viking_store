<html>
  <head>
    <meta charset="utf-8">
    <title>Viking Store Admin Portal</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/master.css">
    <!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script> -->
  </head>
  <body>
    <div id="masthead">
      <div class="container">
          <div class="row">
            <div class="col-md-7">
              <h1>Viking Store Admin Portal</h1>
            </div>
          </div>
      </div><!--/container-->
    </div><!--/masthead-->

    <!--main-->
    <div class="container">
    	<div class="row">
          <!--left-->
          <div class="col-md-3" id="leftCol">
            <h3>Navigation Options</h3>
            <ul class="nav nav-stacked" id="sidebar">
              <li><a href="#sec0">Users</a></li>
              <li><a href="#sec1">Addresses</a></li>
              <li><a href="#sec2">Orders</a></li>
              <li><a href="#sec3">Products</a></li>
              <li><a href="#sec4">Categories</a></li>
              <li><hr></li>
              <li><%= link_to("Analytics Dashboard", '#')%></li>
            </ul>
          </div><!--/left-->
          <!--right-->
          <div class="col-md-9">
            <h2 id="heading-users" class="text-center">Viking Store Users</h2>
            <br class="text-center">
              <a class="btn btn-primary" href="#" role="button">Create New User</a></button>
            </br>
            <% if flash[:notice] %>
              <div class="notice"><%= flash[:notice] %></div>
            <% end %>
            <table class="table table-striped table-bordered">
              <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Joined</th>
                <th>City</th>
                <th>State</th>
                <th>Orders</th>
                <th>Last Order Date</th>
                <th>SHOW</th>
                <th>EDIT</th>
                <th>DELETE</th>
              </tr>
              <% 10.times do %>
                <tr>
                  <% 10.times do %>
                    <td></td>
                  <% end %>
                </tr>
                <% end %>
              </table>




            </div><!--/right-->
      	</div><!--/row-->
    </div><!--/container-->


  </body>
</html>
