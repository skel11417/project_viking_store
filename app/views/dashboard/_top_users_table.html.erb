<table class="table table-striped table-bordered">
  <tr>
    <th>Item</th>
    <th>User</th>
    <th>Quantity</th>
  </tr>

    <% data_array.each do |line| %>
      <tr>
        <td><%= line[0] %> </td>
        <td><%= "#{line[1].first_name} #{line[1].last_name}" %></td>
        <td><%= line[1].total %> </td>
      </tr>
    <% end %>

</table>
